{
  "name": "kjanczyk/pimcore-soml-bundle",
  "description": "Pimcore Social Media Library Bundle: import and manage social media feed",
  "type": "pimcore-bundle",
  "license": "MIT",
  "keywords": ["pimcore", "bundle", "social media", "import", "feed"],
  "authors": [
    {
      "name": "Kacper Janczyk",
      "email": "kacper@janczyk.dev"
    }
  ],
  "require": {
    "php": ">=8.2",
    "pimcore/pimcore": "^11.0",
    "guzzlehttp/guzzle": "^7.8",
    "nesbot/carbon": "^3.0",
    "symfony/filesystem": "^6.4 || ^7.0"
  },
  "autoload": {
      "psr-4": {
        "KJanczyk\\PimcoreSOMLBundle\\": "src/"
      }
  },
  "autoload-dev": {
    "psr-4": {
        "KJanczyk\\PimcoreSOMLBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        {
          "id": "KJanczyk\\PimcoreSOMLBundle\\PimcoreSOMLBundle"
        }
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
